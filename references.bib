@Article{Nicholson2015a,
  author =        {{Nicholson}, T.~L. and {Campbell}, S.~L. and {Hutson}, R.~B. and {Marti}, G.~E. and {Bloom}, B.~J. and {McNally}, R.~L. and {Zhang}, W. and {Barrett}, M.~D. and {Safronova}, M.~S. and {Strouse}, G.~F. and {Tew}, W.~L. and {Ye}, J.},
