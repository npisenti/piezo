@TechReport{Seifert2009a,
  author =      {Seifert, Frank},
  title =       {Resistor Current Noise Measurements},
  institution = {LIGO},
  year =        {2009},
  number =      {LIGO-T0900200},
  file =        {:Seifert2009a - Resistor Current Noise Measurements.pdf:PDF},
  review =      {Current noise (distinct from Johnson thermal noise) is measured in several different resistors. General obeservations:

* Larger phyiscal size -> lower current noise
* In some resisotrs, current noise dominates over johnson noise
* Vishay TNPW 0.1% resistors do pretty damn well!

* Current noise in a resistor scales with the current across the junction
* Typically it has a 1/f PSD, and "rounds off at the point where it equals the thermal noise" -- eg, white noise floor is given by johnson noise; current noise dominates like 1/f at low frequencies.
* Magnitude depends heavily on type of resistor/technology (eg, thin film, thick film, etc.)
* Define "noise quality index", which is the RMS voltage noise divided by the DC voltage, in a single decade (fun fact: 1/f noise has constant RMS power in any decade!) -- eg, 0dB noise index = 1uV/V per decade.},
  url =         {https://dcc.ligo.org/LIGO-T0900200/public}
}

